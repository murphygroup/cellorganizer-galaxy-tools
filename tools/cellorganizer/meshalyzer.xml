<tool id="meshalyzer" name="Validates " version="0.0.1">
  <description>Wavefront OBJ file.</description>
  <command>
  	echo ln -s $dataset $(pwd)/file.obj
  	ln -s $dataset $(pwd)/file.obj
  	meshalyzer ./file.obj
  </command>
  <inputs>
	  <param name="dataset" type="data" format="obj" label="Choose a file for validation" multiple="false" optional="false" />      
  </inputs>
  <outputs>
    <data name="output" from_work_dir="file.obj"/>
  </outputs>
<help>
**Description**

Validate Wavefront OBJ file using meshalyzer.
</help>
</tool>
