<tool id="download_and_validation" name="download and validation" version="1.0.0">
  <description>from an image url</description>
  <command interpreter="bash">
    download_and_validation.sh $image_name 
  </command>
  <inputs>
    <param name="image_name" type="text" value="http://woodstock.compbio.cs.cmu.edu:8080/job/cellorganizer-demo3D34/166/artifact/demos/3D/demo3D34/img/cell1/cell1.ome.tif" label="Provide an image url:"/>
  </inputs>
  <outputs>
    <data format="tiff" name="image" from_work_dir="image.ome.tif"/>
  </outputs>
</tool>
